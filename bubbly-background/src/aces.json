{
	"main": {
		"conditions": [
			{
				"id": "is-animate",
				"scriptName": "IsAnimate",
				"highlight": false
			},
			{
				"id": "is-compose-lighter",
				"scriptName": "IsComposeLighter",
				"highlight": false
			}
		],
		"actions": [
			{ "id": "reset-property",
				"scriptName": "resetProperty",
				"highlight": false
			},
			{ "id": "redraw-background",
				"scriptName": "redrawBackground",
				"highlight": false
			}
		],
		"expressions": []
	},
	"setproperty": {
		"actions": [
			{	"id": "change-style-background",
				"scriptName": "changeStyleBackground",
				"highlight": false,
				"params": [
						{	"id": "set-style",
							"type": "combo",
							"initialValue": "bubbly",
							"items": ["solid","gradient","bubbly"]
						}
					]
			},
			{	"id": "change-preset-bubbly",
				"scriptName": "changePresetBubbly",
				"highlight": false,
				"params": [
					{	"id": "set-preset",
						"type": "combo",
						"initialValue": "blueWithWhiteBubbles",
						"items": ["custom",
										"blueWithWhiteBubbles",
										"blackRedWithRedBubbles",
										"purpleWithMulticoloredBubbles",
										"yellowPinkWithRedOrangeYellowBubbles"]
					}
				]
			},
			{ "id": "set-animate",
				"scriptName": "setAnimate",
				"highlight": false,
				"params": [
					{	"id": "animate",
						"type": "combo",
						"initialValue": "animate",
						"items": [ "notAnimate", "animate"	]
					}
				]
			},
			{ "id": "set-compose",
				"scriptName": "setCompose",
				"highlight": false,
				"params": [
					{	"id": "compose",
						"type": "combo",
						"initialValue": "lighter",
						"items": [ "sourceOver", "lighter"	]
					}
				]
			},
			{ "id": "set-blur",
				"scriptName": "setBlur",
				"highlight": false,
				"params": [
					{	"id": "number",
						"type": "number",
						"initialValue": 4
					}
				]
			},
			{ "id": "set-bubbles",
				"scriptName": "setBubbles",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"Math.floor((canvas.width + canvas.height) * 0.02)\""
					}
				]
			},
			{ "id": "set-bubble-func",
				"scriptName": "setBubbleFunc",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"() => `hsla(0, 0%, 100%, ${r() * 0.1})`\""
					}
				]
			},
			{ "id": "set-angle-func",
				"scriptName": "setAngleFunc",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"() => Math.random() * Math.PI * 2\""
					}
				]
			},
			{ "id": "set-velocity-func",
				"scriptName": "setVelocityFunc",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"() => 0.1 + Math.random() * 0.5\""
					}
				]
			},
			{ "id": "set-radius-func",
				"scriptName": "setRadiusFunc",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"() => 4 + Math.random() * canvas.width / 25\""
					}
				]
			},
			{ "id": "set-color-start",
				"scriptName": "setColorStart",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"#2AE\""
					}
				]
			},
			{ "id": "set-color-stop",
				"scriptName": "setColorStop",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"#17B\""
					}
				]
			},
			{ "id": "set-color-shadow",
				"scriptName": "setColorShadow",
				"highlight": false,
				"params": [
					{	"id": "string",
						"type": "string",
						"initialValue": "\"#FFF\""
					}
				]
			}
		]
	},
